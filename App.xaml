             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainWindow.xaml"
             xmlns:enumerables="clr-namespace:Spending.Enumerables"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
